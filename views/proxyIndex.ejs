<%- include('header.ejs') %>

<h2 class="float-left">Active Proxies</h2>
<a class="btn btn-lg btn-primary float-right" href="/proxies/create">Create Proxy</a>

<br><br><br>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Proxy Name</th>
            <th scope="col">Model</th>
            <th scope="col">Delete</th>
        </tr>
    </thead>
    <tbody>
        <% for (proxy of proxies){ %>
        <tr>
            <td><%= proxy.apiName %></th>
            <td><a href="/proxies/<%= proxy.id %>/model">View Model</a></td>
            <td>x</td>
        </tr>
        <% } %>
    </tbody>
</table>

<%- include('footer.ejs') %>)
